          <ul class="content_block"> 
            {% if section.settings.block_5_icon_1_desc != blank  %}  
            <li style="color:{{ section.settings.block_5_icon_description_color }};" class="desc">{{ section.settings.block_5_icon_1_desc }}</li>
            {% endif %} 
            {% if section.settings.block_5_icon_2_desc != blank %} 
            <li style="color:{{ section.settings.block_5_icon_description_color }};" class="desc">{{ section.settings.block_5_icon_2_desc }}</li>
            {% endif %} 
            {% if section.settings.block_5_icon_3_desc != blank  %}  
            <li style="color:{{ section.settings.block_5_icon_description_color }};" class="desc">{{ section.settings.block_5_icon_3_desc }}</li>
