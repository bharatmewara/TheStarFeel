<style>


  .about-us .team-wrapper li a{color:{{section.settings.team_socialiconcolor}};}

  .about-us .team-wrapper h6{color:{{section.settings.team_socialiconcolor}};}

  .about-us .team-wrapper h6::after {background: {{section.settings.team_socialhoverborder}} ;}

  .about-us .team-wrapper li a:hover{color:{{section.settings.team_socialhoverborder}};}

  .about-us .team-wrapper:hover{}

  .about-us .team-wrapper:hover h5{color:{{section.settings.team_socialhoverborder}};}

  .about-us .block1 .content_section {border: 1px solid {{section.settings.block_1_icontitleborder}};}

  .about-us .block1 .support_icon {background: {{section.settings.block_1_iconbg}};}


  .about-us .dt-sc-toggle-accordion ,  .about-us .dt-sc-toggle-accordion.active{background:{{section.settings.toggle_titlebgcolor}};}


  .about-us  .dt-sc-toggle-accordion.active  a ,  .about-us  .dt-sc-toggle-accordion  a{color:{{section.settings.toggle_titlecolor}};}


  .about-us .dt-sc-toggle-frame-set .dt-sc-toggle-accordion::before { background: {{section.settings.toggle_iconbg }};color:{{section.settings.toggle_iconcolor}};}

  .about-us .border-title-left::before,.about-us .border-title-left::after , .about-us .border-title::after, .about-us .border-title::before{ background: {{section.settings.title_border_color}};}

  .about-us .intro-video .video-player{background: {{section.settings.video_btnbg }};color:{{section.settings.video_btncolor}};}

  .about-us .intro-video .video-player:hover{background:{{section.settings.btn_bg_color}};color:{{section.settings.btn_txt_color}};}

  .about-us .dt-sc-toggle-frame .dt-sc-toggle-content{background:{{section.settings.toggle_descbg}};}


  .about-us .btn:hover{background:{{section.settings.btn_bg_color}}!important;color:{{section.settings.btn_txt_color}}!important;}

  .about-us .block1 .support_section h4::before ,.about-us .block1 .support_section h4::after{ background: {{section.settings.block_1_icontitleborder}} ;}

  .about-us .block1 .content_section:hover{border:1px solid {{section.settings.block_1_iconhover}};}

  .about-us .block1 .content_section:hover .support_icon {background:{{section.settings.block_1_iconhover}};}

  .about-us .block1 .content_section:hover .support_section h4::before {background:{{section.settings.block_1_iconhover}};}

  .about-us .block1  .content_section:hover .support_section h4::after{background:{{section.settings.block_1_iconhover}};}

  .about-us .block1 .content_section:hover .support_icon{color:{{section.settings.block_1_icontxthover}}!important;}

  .about-us .block1 .support_icon{border: 10px solid #fff;}

  .about-us .block1 .support_section h4::before { box-shadow: 0 0 0 5px #fff;}

  .about-us .team-img::after,.about-us .team-img::before{background:{{section.settings.team_overlaybgcolor}};}

  .about-us .social-icon a{background:{{section.settings.team_socialiconbgcolor}};}

  .about-us .social-icon a:hover{background:{{section.settings.socialiconhovebgrcolor}};color:{{section.settings.socialiconhovercolor}};}
  .about-us .block5 .desc::after {background: {{section.settings.block_5_icon_description_color}};}
  
</style>


