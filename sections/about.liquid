        {% if section.settings.right_img != blank %}
        <div class="grid__item wide--one-half post-large--one-half large--one-half medium--one-half small-grid__item ">
          <div class="right-img">
            <img src="{{ section.settings.right_img | img_url: '1024x1024' }}" alt="" title="" /> 
          </div>
        </div>
        {% endif %}
      </div>
    </div>
  </div>
