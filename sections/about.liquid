
<div data-section-id="{{ section.id }}"  data-section-type="about-us" class="about-us">
  
{% if section.settings.show_1_section != blank %}
  <div class="grid-uniform block5"> 
    <div class="container">   
      <div class="grid__item">
        
     {% if section.settings.show_1_leftsection != blank  %} 
        <div class="grid__item wide--one-half post-large--one-half large--one-half medium--one-half small-grid__item wow fadeInUp"  data-wow-delay="{{ settings.time_lazy_load }}ms">
          <div class="content_block"> 
            <div class="content_section">                                
              <div class="support_text">
                {% if section.settings.block_5_heading != blank  %} 
                <div class="border-title text-left">         
                  <h2 style="color:{{ section.settings.block_5_heading_color }};">{{ section.settings.block_5_heading }}</h2>                 
                  <span  style="background:{{ section.settings.block_5_headingborder_color }};" class="sep"></span>
                </div>
                {% endif %}
                {% if section.settings.block_5_description != blank  %}  
                <p  class="des" style="color:{{ section.settings.block_5_description_color }};">{{ section.settings.block_5_description }}</p>
                {% endif %} 
                {% if section.settings.block_5_maindesc != blank  %}  
                <p style="color:{{ section.settings.block_5_maindesc_color }};">{{ section.settings.block_5_maindesc }}</p>
                {% endif %} 
                {% if section.settings.block_5_subdesc != blank  %}  
                <p style="color:{{ section.settings.block_5_subdesc_color }};">{{ section.settings.block_5_subdesc }}</p>
                {% endif %} 
              </div>      
            </div>    
